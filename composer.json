{
    "name": "scott-nason/eco-helpers",
    "description": "Eco Helpers is an overlay to Laravel that aids in rapid application development for security controlled CRUD applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Nason",
            "email": "scott@nasonproductions.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": ">= 8.2.0"
    },

    "autoload": {
        "psr-4": {
            "ScottNason\\EcoHelpers\\": "src/",
            "Database\\Migrations\\": "src/database/migrations/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "ScottNason\\EcoHelpers\\EcoHelpersServiceProvider"
            ],
            "dont-discover": []
        }
    }
}
